@model IEnumerable<DeliveryOnline.Models.Producto>
@{
    ViewBag.Title = "ProductoByTienda";
}

<h2>Productos</h2>

@foreach (var v in Model)
<div>
    <div>
        <div>@Html.ActionLink(v.Descripcion, "Details", new { id = v.CodigoId })</div>
        <div>
            <div>Precio</div>
            <div>@Html.ActionLink("Agregar al Carrito", "Carrito")</div>
        </div>
    </div>
</div>

@Html.ActionLink("Volver al Inicio", "UserIndex", "Tienda")